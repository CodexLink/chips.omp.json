{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:c-badge-folder",
          "foreground": "p:c-badge-text",
          "leading_diamond": "\ue0b6",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": "\uf07b {{ .Path }}",
          "trailing_diamond": "\ue0b4",
          "type": "path"
        },
        {
          "type": "git",
          "style": "diamond",
          "foreground": "p:c-badge-text",
          "background": "p:c-git-normal",
          "background_templates": [
            "{{ if gt .Ahead 0 }}p:c-git-ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:c-git-behind{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:c-git-ahead-behind{ end }}",
            "{{ if and (not .Working.Changed) (.Staging.Changed)}}p:c-git-staging{{ end }}",
            "{{ if and (.Working.Changed) (.Staging.Changed) }}p:c-git-staging-working{{ end }}",
            "{{ if .UpstreamGone }}p:c-git-upstream-gone{{ end }}",
            "{{ if and (.Working.Changed) (not .Staging.Changed)}}p:c-git-working{{ end }}"
          ],
          "leading_diamond": " \ue0b6",
          "template": "{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \ue0b1 \uF448 ({{ .Working.String }}){{ end }}{{ if and .Working.Changed .Staging.Changed }} \ue621 {{ end }}{{ if .Staging.Changed }}{{ if not .Working.Changed }} \ue0b1 {{ end }}\uF854 ({{ .Staging.String }}){{ end }}{{ if .StashCount }} \ue621 \uF6FC {{ .StashCount }} Stash{{ if gt .StashCount 1 }}es{{ end }}{{ end }}",
          "properties": {
            "branch_icon": "\uE0A0 ",
            "branch_ahead_icon": "(\ufa9b)",
            "branch_behind_icon": "(\uf9aa)",
            "branch_gone_icon": "(\uf00d)",
            "branch_identical_icon": "(\uf00c)",
            "fetch_status": true,
            "fetch_stash_count": true
          },
          "trailing_diamond": "\ue0b4"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background_templates": [
            "{{ if lt .Ms 1000 }}p:c-exec-faster{{ end }}",
            "{{ if lt .Ms 60000 }}p:c-exec-fast{{ end }}",
            "{{ if lt .Ms 3600000 }}p:c-exec-normal{{ end }}",
            "{{ if lt .Ms 10800000 }}p:c-exec-slow{{ end }}",
            "{{ if ge .Ms 10800001 }}p:c-exec-slower{{ end }}"
          ],
          "foreground": "p:c-badge-text",
          "leading_diamond": "\uE0B6",
          "properties": {
            "style": "austin",
            "threshold": 1
          },
          "style": "diamond",
          "template": "\uFA1E {{ .FormattedMs }}.",
          "trailing_diamond": "\uE0B4",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "diamond",
          "template": "{{if .Root }}<p:c-badge-return-success,>\ue0b6</><p:c-badge-text,p:c-badge-return-success>#</><p:c-badge-return-success,>\ue0b4</> {{ end }}\u276F"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "wakatime",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#007acc",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\uE0B4",
          "properties": {
            "url": "https://wakatime.com/api/v1/users/current/summaries?start=today&end=today&api_key={{ .Env.WAKATIME_API_KER }}",
            "cache_timeout": 10,
            "http_timeout": 10000
          },
          "template": "{{ if .Segments.Wakatime }} {{ secondsRound .CummulativeTotal.Seconds }} {{ else }} x {{ end }}"
        },
        {
          "background_templates": [
            "{{ if regexMatch \"(^(0[6-9])|(1[0-1])):\" (.CurrentDate | date .Format) }}p:c-date-morning{{ end }}",
            "{{ if contains \"12:\" (.CurrentDate | date .Format) }}p:c-date-noon{{ end }}",
            "{{ if regexMatch (\"(^1[3-7]):\") (.CurrentDate | date .Format) }}p:c-date-afternoon{{ end }}",
            "{{ if regexMatch \"(^(1[8-9])|(2[0-3])):\" (.CurrentDate | date .Format) }}p:c-date-evening{{ end }}",
            "{{ if regexMatch \"^(0[0-5]):\" (.CurrentDate | date .Format) }}p:c-date-night{{ end }}"
          ],
          "foreground": "p:c-badge-text",
          "leading_diamond": "{{ if not .Segments.Wakatime }}\ue0b6{{ else }}x {{ end }}",
          "properties": {
            "time_format": "15:04 (01/02)"
          },
          "style": "diamond",
          "template": "{{ .CurrentDate | date .Format }}",
          "trailing_diamond": "\ue0b4",
          "type": "time"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{ if .Root }}# (as {{ .UserName }}) | {{ end }}{{ if .WSL }}WSL | {{ end }}{{ .Folder }} ({{ .Shell }})",
  "final_space": true,
  "palette": {
    "c-badge-text": "#212121",
    "c-badge-folder": "#FFD770",
    "c-badge-return-custom": "#FFA06D",
    "c-badge-return-success": "#B2FF59",
    "c-badge-return-fail-term": "#FF8A80",
    "c-date-morning": "#FFFF8D",
    "c-date-noon": "#FFF64F",
    "c-date-afternoon": "#FFC400",
    "c-date-evening": "#C0CFFF",
    "c-date-night": "#83B9FF",
    "c-exec-faster": "#76FFFF",
    "c-exec-fast": "#C6FF00",
    "c-exec-normal": "#FFFF00",
    "c-exec-slow": "#FFD180",
    "c-exec-slower": "#FF867F",
    "c-git-ahead": "#6EFFFF",
    "c-git-behind": "#FFA06D",
    "c-git-ahead-behind": "#FFD740",
    "c-git-normal": "#66FFA6",
    "c-git-staging": "#FFB2FF",
    "c-git-staging-working": "#A7FFEB",
    "c-git-upstream-gone": "#FF9E80",
    "c-git-working": "#84FFFF",
    "c-secondary-ellipsis": "#FFFF8D"
  },
  "transient_prompt": {
    "template": "\ue0b6 {{ if .Segments.Executiontime.Ms }}\uf9ad\ufa1e {{ .Segments.Executiontime.FormattedMs }} \ue0bd {{ end }}\ufb0c <b>{{ if eq .Code 0 }}OK{{ else if eq .Code 1 }}FAIL{{ else if eq .Code 130 }}TERM{{ else }}Code{{ end }} ({{ .Code }})</b> \ue0b4 \u276F "
  },
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "p:c-secondary-ellipsis",
    "template": " ... "
  },
  "version": 2
}
