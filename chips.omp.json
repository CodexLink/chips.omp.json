{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:c-badge-folder",
          "foreground": "p:c-badge-text",
          "leading_diamond": "\uE0B6",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": "\uF07B {{ .Path }}",
          "trailing_diamond": "\uE0B4",
          "type": "path"
        },
        {
          "background": "p:c-git-normal",
          "background_templates": [
            "{{ if gt .Ahead 0 }}p:c-git-ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:c-git-behind{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:c-git-ahead-behind{{ end }}",
            "{{ if and (not .Working.Changed) (.Staging.Changed)}}p:c-git-staging{{ end }}",
            "{{ if and (.Working.Changed) (.Staging.Changed) }}p:c-git-staging-working{{ end }}",
            "{{ if .UpstreamGone }}p:c-git-upstream-gone{{ end }}",
            "{{ if and (.Working.Changed) (not .Staging.Changed)}}p:c-git-working{{ end }}"
          ],
          "foreground": "p:c-badge-text",
          "leading_diamond": " \uE0B6",
          "template": "{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \ue0b1 \uF448 ({{ .Working.String }}){{ end }}{{ if and .Working.Changed .Staging.Changed }} \ue621 {{ end }}{{ if .Staging.Changed }}{{ if not .Working.Changed }} \ue0b1 {{ end }}\uF854 ({{ .Staging.String }}){{ end }}{{ if .StashCount }} \ue621 \uF6FC {{ .StashCount }} Stash{{ if gt .StashCount 1 }}es{{ end }}{{ end }}",
          "properties": {
            "branch_icon": "\uE0A0 ",
            "branch_ahead_icon": "(\uFA9B)",
            "branch_behind_icon": "(\uf9aa)",
            "branch_gone_icon": "(\uf00d)",
            "branch_identical_icon": "(\uF00C)",
            "fetch_status": true,
            "fetch_stash_count": true
          },
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background_templates": [
            "{{ if lt .Ms 1000 }}p:c-exec-faster{{ end }}",
            "{{ if lt .Ms 60000 }}p:c-exec-fast{{ end }}",
            "{{ if lt .Ms 3600000 }}p:c-exec-normal{{ end }}",
            "{{ if lt .Ms 10800000 }}p:c-exec-slow{{ end }}",
            "{{ if gt .Ms 10800001 }}p:c-exec-slower{{ end }}"
          ],
          "foreground": "p:c-badge-text",
          "leading_diamond": "\uE0B6",
          "properties": {
            "style": "austin",
            "threshold": 1
          },
          "style": "diamond",
          "template": "\uFA1E {{ .FormattedMs }}.",
          "trailing_diamond": "\uE0B4 ",
          "type": "executiontime"
        },
        {
          "background_templates": [
            "{{ if lt (.CummulativeTotal.Seconds | toDecimal) 3600 }}p:c-wakatime-undertime{{ end }}",
            "{{ if and (gt (.CummulativeTotal.Seconds | toDecimal) 3601) (lt (.CummulativeTotal.Seconds | toDecimal) 10800) }}p:c-wakatime-warm-up{{ end }}",
            "{{ if and (gt (.CummulativeTotal.Seconds | toDecimal) 10801) (lt (.CummulativeTotal.Seconds | toDecimal) 25200) }}p:c-wakatime-working{{ end }}",
            "{{ if and (gt (.CummulativeTotal.Seconds | toDecimal) 25201) (lt (.CummulativeTotal.Seconds | toDecimal) 28800) }}p:c-wakatime-quota{{ end }}",
            "{{ if gt (.CummulativeTotal.Seconds | toDecimal) 28801 }}p:c-wakatime-overtime{{ end }}"
          ],
          "foreground": "p:c-badge-text",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B4 ",
          "properties": {
            "cache_timeout": 5,
            "http_timeout": 10000,
            "url": "https://wakatime.com/api/v1/users/current/summaries?start=today&end=today&api_key={{ .Env.WAKATIME_API_KEY }}"
          },
          "style": "diamond",
          "template": "{{ if and (.Env.WAKATIME_API_KEY)  (not (.Env.SEGMENT_DISABLE_WAKATIME | toDecimal)) }}\uFA19 {{ secondsRound .CummulativeTotal.Seconds }}.{{ end }}",
          "type": "wakatime"
        },
        {
          "background_templates": [
            "{{ if regexMatch \"(^(0[6-9])|(1[0-1])):\" (.CurrentDate | date .Format) }}p:c-date-time-morning{{ end }}",
            "{{ if regexMatch (\"(^1[2-4]):\") (.CurrentDate | date .Format) }}p:c-date-time-noon{{ end }}",
            "{{ if regexMatch (\"(^1[5-7]):\") (.CurrentDate | date .Format) }}p:c-date-time-afternoon{{ end }}",
            "{{ if regexMatch \"(^(1[8-9])|(2[0-3])):\" (.CurrentDate | date .Format) }}p:c-date-time-evening{{ end }}",
            "{{ if regexMatch \"^(0[0-5]):\" (.CurrentDate | date .Format) }}p:c-date-time-night{{ end }}"
          ],
          "foreground": "p:c-badge-text",
          "leading_diamond": "\uE0B6",
          "properties": {
            "time_format": "15:04 (01/02)"
          },
          "style": "diamond",
          "template": "{{ if not (.Env.SEGMENT_DISABLE_DTIME | toDecimal) }}\uF5EF {{ .CurrentDate | date .Format }}{{ end }}",
          "trailing_diamond": "\uE0B4",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "p:c-project-python-color",
          "leading_diamond": "\uE0B6",
          "foreground": "p:c-badge-text",
          "properties": {
            "display_mode": "context",
            "fetch_virtual_env": true
          },
          "style": "diamond",
          "template": "{{ if not (.Env.SEGMENT_DISABLE_PROJECT_PL | toDecimal) }}\uE235 {{ .Full }} {{ if .Venv }}\uE621 {{ .Venv }}{{ end }}{{ end }}",
          "trailing_diamond": "\uE0B4 ",
          "type": "python"
        },
        {
          "type": "text",
          "style": "diamond",
          "template": "{{if .Root }}<p:c-badge-return-success,p:c-badge-text>\uE0B6</><p:c-badge-text,p:c-badge-return-success>#</><p:c-badge-return-success,>\uE0B4</> {{ end }}<p:c-badge-white>\u276F</>"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{ if .Root }}# (as {{ .UserName }}) | {{ end }}{{ if .WSL }}WSL | {{ end }}{{ .Folder }} ({{ .Shell }})",
  "final_space": true,
  "palette": {
    "c-badge-white": "#FAFAFA",
    "c-badge-text": "#212121",
    "c-badge-folder": "#FFD770",
    "c-badge-return-custom": "#FFA06D",
    "c-badge-return-success": "#B2FF59",
    "c-badge-return-fail-term": "#FF8A80",
    "c-date-time-morning": "#FFFF8D",
    "c-date-time-noon": "#FFF64F",
    "c-date-time-afternoon": "#FFC400",
    "c-date-time-evening": "#C0CFFF",
    "c-date-time-night": "#83B9FF",
    "c-exec-faster": "#76FFFF",
    "c-exec-fast": "#C6FF00",
    "c-exec-normal": "#FFFF00",
    "c-exec-slow": "#FFD180",
    "c-exec-slower": "#FF867F",
    "c-git-ahead": "#6EFFFF",
    "c-git-behind": "#FFA06D",
    "c-git-ahead-behind": "#FFD740",
    "c-git-normal": "#66FFA6",
    "c-git-staging": "#FFB2FF",
    "c-git-staging-working": "#A7FFEB",
    "c-git-upstream-gone": "#FF9E80",
    "c-git-working": "#84FFFF",
    "c-project-python-color": "#FFE873",
    "c-secondary-ellipsis": "#FFFF8D",
    "c-wakatime-undertime": "#A7FFEB",
    "c-wakatime-warm-up": "#FFFFB3",
    "c-wakatime-working": "#FFD180",
    "c-wakatime-quota": "#FFD0B0",
    "c-wakatime-overtime": "#FF8A80"
  },
  "transient_prompt": {
    "template": "<{{ if eq .Code 0 }}p:c-badge-return-success{{ else if or (eq .Code 1) (eq .Code 130) }}p:c-badge-return-fail-term{{ else }}p:c-badge-return-custom{{ end }}>\uE0B6</><p:c-badge-text,{{ if eq .Code 0 }}p:c-badge-return-success{{ else if or (eq .Code 1) (eq .Code 130) }}p:c-badge-return-fail-term{{ else }}p:c-badge-return-custom{{ end }}>{{ if .Segments.Executiontime.Ms }}{{ if not (empty .Env.SEGMENT_DISABLE_TRANSIENT_RECENT_EXEC_TIME) }}{{ if regexMatch \"^[Ff](alse)$\" (title .Env.SEGMENT_DISABLE_TRANSIENT_RECENT_EXEC_TIME) }}\uF55D \uFA1E {{ .Segments.Executiontime.FormattedMs }} \uE621 {{ end }}{{ end }}{{ end }}\uFB0C <b>{{ if eq .Code 0 }}OK{{ else if eq .Code 1 }}FAIL{{ else if eq .Code 130 }}TERM{{ else }}Code{{ end }} ({{ .Code }})</b></><{{ if eq .Code 0 }}p:c-badge-return-success{{ else if or (eq .Code 1) (eq .Code 130) }}p:c-badge-return-fail-term{{ else }}p:c-badge-return-custom{{ end }}>\uE0B4</> \u276F "
  },
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "p:c-secondary-ellipsis",
    "template": " ... "
  },
  "version": 2
}
